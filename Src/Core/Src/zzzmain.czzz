/**
  * @brief  The application entry point.
  * @retval int
  */
int main(void)
{

  /* USER CODE BEGIN 1 */

  /* USER CODE END 1 */

  /* MCU Configuration--------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */
  HAL_ICACHE_Disable();
  PCLK1Freq = HAL_RCC_GetPCLK1Freq();
  HAL_ICACHE_Enable();
  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  MX_ADC1_Init();
  MX_ICACHE_Init();
  MX_USART3_UART_Init();
  MX_RTC_Init();
  MX_SPI3_Init();
  /* USER CODE BEGIN 2 */

  /* init code for FATFS */
  MX_FATFS_Init();
  /* Mount SD Card */
  if(f_mount(&fs, "", 0) != FR_OK)
  {
    RedLed(LED_EBLINK_1);
  }

  (void)GetSettings();

  lVersion = VERSION;
  lSerialNumber = MakeSerialNumber();
  strcpy(szBuildDateVar, __DATE__);

  // crashes in the interrupt if TI_NONE is selected
  MainTrackConfig();

  GreenLed(LED_NORMAL);

  /* USER CODE END 2 */

  /* Init scheduler */
  osKernelInitialize();

  /* Call init function for freertos objects (in app_freertos.c) */
  MX_FREERTOS_Init();

  /* Start scheduler */
  osKernelStart();

  /* We should never get here as control is now taken by the scheduler */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */
  while (1)
  {
    /* USER CODE END WHILE */

    /* USER CODE BEGIN 3 */
  }
  /* USER CODE END 3 */
}

